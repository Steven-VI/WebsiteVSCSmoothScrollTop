{
  "name": "websitevscsmoothscrolltop",
  "version": "1.0.0",
  "description": "smooth scroll",
  "main": "script.js",
  "scripts": {
    "lint": "jshint **",
    "build:watch": "watch 'npm run build' .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:js": "browserify assets/scripts/script.js > dist/scripts/script.js",
    "build:css": "stylus assets/styles/site.styl -o dist/styles/site.css",
    "build:html": "jade assets/html/index.jade -o dist/",
    "build": "npm run build:js && npm run build:css && npm run build:html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Steven-VI/WebsiteVSCSmoothScrollTop.git"
  },
  "keywords": [
    "smooth",
    "scroll"
  ],
  "author": "Steven VI",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Steven-VI/WebsiteVSCSmoothScrollTop/issues"
  },
  "homepage": "https://github.com/Steven-VI/WebsiteVSCSmoothScrollTop#readme",
  "dependencies": {
    "browserify": "^16.5.0",
    "jade": "^1.11.0",
    "jshint": "^2.10.2",
    "stylus": "^0.54.7"
  }
}
